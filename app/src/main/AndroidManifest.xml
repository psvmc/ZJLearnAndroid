<?xml version="1.0" encoding="utf-8"?>
<manifest package="cn.psvmc.zjlearnandroid"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".C.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".C.TabbarActivity">
        </activity>
        <activity
            android:name=".C.FloatingActionButtonActivity"
            android:label="@string/title_activity_floating_action_button"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>